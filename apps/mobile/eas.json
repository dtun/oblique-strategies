{
	"cli": {
		"version": ">= 14.0.0",
		"appVersionSource": "remote"
	},
	"build": {
		"production": {
			"distribution": "store",
			"ios": {
				"resourceClass": "m-medium",
				"buildConfiguration": "Release"
			},
			"env": {
				"EXPO_PUBLIC_ENV": "production"
			},
			"channel": "production"
		},
		"preview": {
			"distribution": "internal",
			"ios": {
				"simulator": true
			},
			"channel": "preview"
		}
	},
	"submit": {
		"production": {
			"ios": {
				"ascAppId": "6757136362"
			}
		}
	},
	"update": {
		"production": {
			"channel": "production"
		}
	}
}
